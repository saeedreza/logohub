{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "docs/.vitepress/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*\\.(css|js|ico|svg|png|jpg|jpeg|gif|woff|woff2))",
      "dest": "/docs/.vitepress/dist/$1"
    },
    {
      "src": "/",
      "dest": "/docs/.vitepress/dist/index.html"
    },
    {
      "src": "/(?!api)(.*)",
      "dest": "/docs/.vitepress/dist/$1.html"
    }
  ]
} 